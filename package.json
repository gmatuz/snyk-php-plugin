{
  "name": "snyk-php-plugin",
  "description": "Snyk CLI PHP plugin",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "build-watch": "tsc -w",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "semantic-release": "semantic-release",
    "test": "npm run lint && npm run unit-test",
    "unit-test": "tap -Rspec test/*.test.ts --timeout=300"
  },
  "files": [
    "lib"
  ],
  "homepage": "https://github.com/snyk/snyk-php-plugin",
  "repository": {
    "type": "git",
    "url": "https://github.com/snyk/snyk-php-plugin"
  },
  "author": "snyk.io",
  "license": "Apache-2.0",
  "engines": {
    "node": ">=6"
  },
  "dependencies": {
    "@snyk/composer-lockfile-parser": "1.0.1"
  },
  "devDependencies": {
    "@types/debug": "4.1.4",
    "@types/lodash": "4.14.133",
    "@types/node": "6.14.6",
    "lodash": "^4.17.5",
    "sync-request": "3.0.1",
    "tap": "14.2.2",
    "tslib": "1.9.3",
    "tslint": "5.17.0",
    "typescript": "3.5.1"
  }
}
