{
  "name": "snyk-php-plugin",
  "description": "Snyk CLI PHP plugin",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "build-watch": "tsc -w",
    "lint": "eslint 'lib/**/*.ts' && (cd test && eslint '**/*.ts')",
    "prepare": "npm run build",
    "semantic-release": "semantic-release",
    "test": "npm run lint && npm run unit-test",
    "unit-test": "tap -Rspec test/*.test.ts --timeout=300"
  },
  "files": [
    "dist"
  ],
  "homepage": "https://github.com/snyk/snyk-php-plugin",
  "repository": {
    "type": "git",
    "url": "https://github.com/snyk/snyk-php-plugin"
  },
  "author": "Snyk <https://snyk.io>",
  "license": "Apache-2.0",
  "engines": {
    "node": ">=8"
  },
  "dependencies": {
    "@snyk/cli-interface": "^2.6.0",
    "@snyk/composer-lockfile-parser": "^1.4.0",
    "tslib": "^1.11.1"
  },
  "devDependencies": {
    "@types/node": "^6.14.6",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "eslint": "^6.8.0",
    "sync-request": "^3.0.1",
    "tap": "^12.7.0",
    "typescript": "^3.8.3"
  }
}
